{
  "name": "astra_backend",
  "version": "1.0.0",
  "description": "Typescript based Nodejs Project",
  "scripts": {
    "build": "export NODE_ENV=production && tsc && tsc-alias",
    "build-win": "set NODE_ENV=production && tsc && tsc-alias",
    "dev_2": "tsc --watch & tsx ./dist/index.js",
    "dev": "export NODE_ENV=development && tsx watch src/index.ts",
    "dev_win": "set NODE_ENV=development && tsx watch src/index.ts",
    "prod": "export NODE_ENV=production && tsx watch src/index.ts",
    "jest": "jest --silent=false",
    "start": "export NODE_ENV=production && tsx dist/src/index.js",
    "start:build": "tsx dist/src/index.js",
    "test": "jest --silent=false",
    "lint": "eslint --fix --ext .js,.ts ./src",
    "prettier-fix": "prettier --write ."
  },
  "dependencies": {
    "@google-cloud/bigquery": "^7.3.0",
    "@types/uuid": "^9.0.8",
    "@wagmi/core": "^1.4.6",
    "axios": "^1.6.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ethers": "5.7.2",
    "express": "^4.18.2",
    "geoip-lite": "^1.4.10",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "localStorage": "^1.0.4",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "node-cron": "^3.0.2",
    "node-fetch": "^3.3.2",
    "snowflake-sdk": "^1.9.0",
    "uuid": "^9.0.1",
    "viem": "^1.18.4"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.4",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/geoip-lite": "^1.4.4",
    "@types/jest": "^29.5.7",
    "@types/lodash": "^4.14.200",
    "@types/mocha": "10.0.1",
    "@types/node": "^20.11.30",
    "@types/node-cron": "^3.0.10",
    "@types/snowflake-sdk": "^1.6.16",
    "@types/supertest": "^2.0.15",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.53.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "6.1.1",
    "jest": "^29.7.0",
    "mocha": "10.2.0",
    "nodemon": "^3.0.1",
    "prettier": "3.0.0",
    "superagent": "^8.1.2",
    "supertest": "6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.8",
